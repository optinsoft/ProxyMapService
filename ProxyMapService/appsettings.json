{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        },
        "Console": {
            "FormatterName": "simple",
            "FormatterOptions": {
                "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
                "UseUtcTimestamp": true
            }
        }
    },
    "AllowedHosts": "*",
    "Authentication": {
        "Jwt": {
            "Enabled": false,
            "Issuer": "",
            "Audience": "",
            "Key": ""
        }
    },
    "ProxyMappings": [
        {
            "Listen": {
                "Port": 5000,
                "RejectHttpProxy": false
            },
            "Authentication": {
                "Required": true,
                "Verify": false,
                "SetHeader":  false,
                "Username": "user",
                "Password": "pass"
            },
            "ProxyServer": {
                "Host": "localhost",
                "Port": 8888
            }
        }
    ]
}
