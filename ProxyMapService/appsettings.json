{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        },
        "Console": {
            "FormatterName": "simple",
            "FormatterOptions": {
                "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
                "UseUtcTimestamp": true
            }
        }
    },
    "AllowedHosts": "*",
    "Authentication": {
        "Jwt": {
            "Enabled": false,
            "Issuer": "",
            "Audience": "",
            "Key": ""
        }
    },
    "ProxyMappings": [
        {
            "Listen": {
                "Port": 5001,
                "RejectHttpProxy": false,
                "StickyProxyLifetime": 0
            },
            "Authentication": {
                "Required": false,
                "Verify": false,
                "SetAuthentication": false,
                "RemoveAuthentication": false,
                "ParseUsernameParameters": false,
                "Username": "user",
                "Password": "pass"
            }
        }
    ],
    "HostStats": {
        "Enabled": false,
        "TrafficStats": false
    },
    "DetailedLogging": {
        "LogReading": false,
        "LogSending": false,
        "LogTrafficData": false,
        "LogSslDecodedData": false,
        "LogStep": false
    },
    "HTTP": {
        "UserAgent": "proxymapper"
    }
}
